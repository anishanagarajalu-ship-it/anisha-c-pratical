#include <stdio.h>

// function to calculate area of rectangle
float area(float length, float breadth)
{
    return length * breadth;
}

int main()
{
    float l, b;

    printf("Enter length: ");
    scanf("%f", &l);

    printf("Enter breadth: ");
    scanf("%f", &b);

    printf("Area of rectangle = %.2f\n", area(l, b));

    return 0;
}
o/p
  Enter length: 5
Enter breadth: 4
Area of rectangle = 20.00
