#include <stdio.h>
#include <string.h>

int main() {
    char items[5][20]; // Array to store 5 item names
    float prices[5];   // Array to store prices of items
    char searchItem[20];
    int i;
    int found = 0;

    // Input items and their prices
    for(i = 0; i < 5; i++) {
        printf("Enter name of item %d: ", i + 1);
        scanf("%s", items[i]);
        printf("Enter price of %s: ", items[i]);
        scanf("%f", &prices[i]);
    }

    // Search for an item
    printf("\nEnter item name to search: ");
    scanf("%s", searchItem);

    for(i = 0; i < 5; i++) {
        if(strcmp(searchItem, items[i]) == 0) {
            printf("%s price = %.2f\n", items[i], prices[i]);
            found = 1;
            break;
        }
    }

    if(!found) {
        printf("%s not found in the shop.\n", searchItem);
    }

    return 0;
}
