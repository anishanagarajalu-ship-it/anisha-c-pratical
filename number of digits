#include <stdio.h>
int main() {
    int number, count = 0;
 printf("Enter a number: ");
    scanf("%d", &number);
 // Count digits
    while (number != 0) {
        number = number / 10;
        count++;
    }

    printf("Number of digits = %d", count);

    return 0;
}
o/p
Enter a number: 45678
Number of digits = 5
