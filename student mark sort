#include <stdio.h>

int main() {
    int n, i, j, temp;

    printf("Enter number of students: ");
    scanf("%d", &n);

    int marks[n];

    // Input marks
    for(i = 0; i < n; i++) {
        printf("Enter marks for student %d: ", i + 1);
        scanf("%d", &marks[i]);
    }

    // Sorting marks in ascending order (using simple bubble sort)
    for(i = 0; i < n - 1; i++) {
        for(j = 0; j < n - i - 1; j++) {
            if(marks[j] > marks[j + 1]) {
                temp = marks[j];
                marks[j] = marks[j + 1];
                marks[j + 1] = temp;
            }
        }
    }

    // Display sorted marks
    printf("\nMarks of students in ascending order:\n");
    for(i = 0; i < n; i++) {
        printf("%d ", marks[i]);
    }

    printf("\n");
    return 0;
}
o/p
Enter number of students: 5
Enter marks for student 1: 78
Enter marks for student 2: 85
Enter marks for student 3: 62
Enter marks for student 4: 90
Enter marks for student 5: 70

Marks of students in ascending order:
62 70 78 85 90

